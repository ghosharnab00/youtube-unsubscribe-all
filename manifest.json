{
    
      "manifest_version": 3,
      "name": "youtube unsubscribe all",
      "description": "the best and easy way to unsubscribe all youtube subscribed channel",
      "version": "1.0",
      "permissions": [
        "activeTab",
        "storage"
      ],
      "background":{
        "service_worker":"background.js"
      },
      "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "run_at": "document_end",

          "js": ["content_script.js"]
        }
      ],
      "action": {
        "default_popup": "index.html",
        "default_icon": "assets/icon128.png",
        "default_title": "youtube unsubscribe all"
      }
    }
    