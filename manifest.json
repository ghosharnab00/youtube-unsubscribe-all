{
  "manifest_version": 3,
  "name": "YouTube Auto Unsubscribe Free",
  "version": "1.0",
  "description": "Automate the process of unsubscribing from YouTube channels without manyally clicking the unsubscribe button.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "assets/icon48.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "96": "assets/icon96.png",
    "128": "assets/icon128.png"
  }
}
